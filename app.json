{
  "expo": {
    "name": "Protein Finder",
    "slug": "protein-finder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#34C759"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.proteinfinder.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "メニュー画像を保存するために写真ライブラリへのアクセスが必要です"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#34C759"
      },
      "package": "com.proteinfinder.app",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "proteinfinder",
    "plugins": ["expo-router"],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "owner": "protein-finder-team"
  }
}
